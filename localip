#!/bin/bash
#
################################
#      _                       #
#  ___| |_ _ __ __ _ ______ _  #
# / __| __| '__/ _` |_  / _` | #
# \__ \ |_| | | (_| |/ / (_| | #
# |___/\__|_|  \__,_/___\__,_| #
#                              #
#------------------------------#
#           localip            #
################################

echo "...Buscando subredes 192.168.x.x"
ip address | grep --color -e "192.168.[0-9]*.[0-9]*" -o
exit 0